package com.craigbooker.java1week3demo;

import android.content.Context;
import android.widget.GridLayout;
import android.widget.TextView;

public class StockDisplay extends GridLayout {

	TextView _symbol;
	TextView _price;
	TextView _time;
	TextView _high;
	TextView _low;
	TextView _change;
	TextView _open;
	TextView _close;	
	TextView _volume;	
	Context _context;
	
	public StockDisplay(Context context) {
		super(context);
		_context = this;
		
		this.setColumnCount(2);
		
	}
	
}

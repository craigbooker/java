package FormStuff;

import android.content.Context;
import android.widget.LinearLayout;


public class FormBorder {
		public FormBorder extends LinearLayout (Context context){
			super(context);
			LinearLayout _ll = new LinearLayout(context);
			LayoutParams _lp = new LayoutParams(LayoutParams.MATCH_PARENT, 5);
			_ll.setLayoutParams(_lp);
			
			//this.addView(_ll);
			return _ll;
		}
}